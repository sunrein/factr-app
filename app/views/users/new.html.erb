<h1> New User </h1>

<%= form_for :user, url: '/users' do |f| %>

  <p>
    <%= f.label :username%><br>
    <%= f.text_field :username %>
  </p>
  <p>
    <%= f.label :email%><br>
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :password%><br>
    <%= f.password_field :password %>
  </p>

    <p class="waves-effect waves-blue btn-flat"><%= f.submit "Register" %>

  </p>
<% end %>
